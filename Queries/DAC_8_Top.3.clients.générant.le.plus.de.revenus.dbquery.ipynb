   "source": [
    "SELECT\n",
    "    c.CustomerID,\n",
    "    SUM(soh.TotalDue) AS RevenusTotaux\n",
    "FROM Customer c\n",
    "JOIN SalesOrderHeader soh\n",
    "    ON c.CustomerID = soh.CustomerID\n",
    "GROUP BY c.CustomerID\n",
    "ORDER BY RevenusTotaux DESC\n",
    "LIMIT 3;"
   ]
  }
