   "source": [
    "SELECT\n",
    "    pc.ProductCategoryID,\n",
    "    pc.Name AS NomCategorie,\n",
    "    COUNT(p.ProductID) AS NombreProduits\n",
    "FROM ProductCategory pc\n",
    "LEFT JOIN Product p\n",
    "    ON pc.ProductCategoryID = p.ProductCategoryID\n",
    "GROUP BY\n",
    "    pc.ProductCategoryID,\n",
    "    pc.Name\n",
    "ORDER BY pc.Name;"
   ]
  }
