   "source": [
    "SELECT\n",
    "    c.CustomerID,\n",
    "    c.LastName,\n",
    "    COUNT(soh.SalesOrderID) AS TotalCommandes\n",
    "FROM Customer c\n",
    "JOIN SalesOrderHeader soh\n",
    "    ON c.CustomerID = soh.CustomerID\n",
    "GROUP BY c.CustomerID, c.LastName;"
